{
  "env": {},
  "configurations": [
    {
      "name": "Butano",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/build",
        "${workspaceFolder}/vendor/butano/butano/include",
        "/opt/devkitpro/libgba/include/**",
        "/opt/devkitpro/devkitARM/include/**",
        "/opt/devkitpro/devkitARM/arm-none-eabi/include/**"
